{
    "name": "mass-killings-front-end",
    "description": "",
    "version": "1.0.0",
    "author": "Mitchell Thorson, USA TODAY",
    "homepage": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/USATODAY/mass-killings-front-end"
    },
    "engines": {
        "node": ">=14.16.0",
        "npm": ">=8.3.2"
    },
    "scripts": {
        "start": "npm run dev",
        "dev": "webpack-dev-server --config ./webpack.config.js --mode development --open",
        "dev:remote-data": "MK_DATA_SRC=remote npm run dev",
        "build": "npm run build:production",
        "build:staging": "DEPLOY_ENV=dev webpack --config ./webpack.config.js --mode production && DEPLOY_ENV=dev npm run embed",
        "build:production": "DEPLOY_ENV=master webpack --config ./webpack.config.js --mode production && DEPLOY_ENV=master npm run embed",
        "deploy": "npm run build && ./scripts/deploy.sh --production --storytelling-embed",
        "deploy:staging": "npm run build:staging && ./scripts/deploy.sh --staging",
        "embed": "node scripts/embed.js",
        "setup": "npm install && node scripts/configure.js && rm -rf .git && git init",
        "fetch-data": "curl https://www.gannett-cdn.com/experiments/usatoday/_data/mass-killings/incidents.json > src/static/data/incidents.json",
        "data:cache": "mkdir -p src/static/data/cache && gsutil rsync gs://experiments-www-gannett-cdn-com/experiments/usatoday/_data/mass-killings/ src/static/data/cache",
        "postinstall": "make && npm run data:cache"
    },
    "devDependencies": {
        "@babel/core": "^7.16.12",
        "@babel/preset-env": "^7.16.11",
        "@google-cloud/storage": "^5.18.0",
        "@types/d3": "^7.4.0",
        "@webcomponents/custom-elements": "^1.5.0",
        "archieml": "^0.5.0",
        "autoprefixer": "^10.4.2",
        "babel-loader": "^8.2.3",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^10.2.1",
        "core-js": "^3.20.3",
        "css-loader": "^6.5.1",
        "css-minimizer-webpack-plugin": "^3.4.1",
        "esm": "^3.2.25",
        "html-webpack-plugin": "^5.5.0",
        "inquirer": "^8.2.0",
        "js-yaml": "^4.1.0",
        "mapshaper": "^0.6.2",
        "mini-css-extract-plugin": "^2.5.2",
        "postcss": "^8.4.5",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^6.2.1",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.6.0",
        "style-loader": "^3.3.1",
        "svelte": "^3.46.2",
        "svelte-loader": "^3.1.2",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.3"
    },
    "dependencies": {
        "d3": "^7.4.4",
        "regenerator-runtime": "^0.13.3",
        "topojson-client": "^3.1.0"
    },
    "browserList": [
        ">1%",
        "last 4 versions",
        "Firefox ESR",
        "not ie < 11"
    ],
    "config": {
        "graphic_slug": "mass-killings-front-end",
        "asset_path": "https://www.gannett-cdn.com/experiments/storytelling-embed/$BRANCH/mass-killings-front-end/"
    }
}
